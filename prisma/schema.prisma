generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://postgres:1234@localhost:5432/augmont"

}

model users{
  id    Int     @id @default(autoincrement())
  email String  @unique
  password String  
}